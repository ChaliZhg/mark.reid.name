---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Inductio Ex Machina</title>
 <link href="http://mark.reid.name/work/blog/atom.xml" rel="self"/>
 <link href="http://mark.reid.name/work/blog/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://mark.reid.name/work/blog/</id>
 <author>
   <name>Mark Reid</name>
   <email>mark@reid.name</email>
 </author>
 
 {% for post in site.categories.work  %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://mark.reid.name/work/blog/{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://mark.reid.name/work/blog/{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>